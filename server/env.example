# Moving Assistant Backend Environment Variables

# Database
DB_PATH=./recognition.db

# Authentication
JWT_SECRET=your_jwt_secret_key_here

# AI Services (existing)
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key

# HERE Maps API (required for Tour Planning v2)
HERE_API_KEY=your_here_api_key_here

# Traccar API (optional - for GPS tracking)
TRACCAR_BASE_URL=https://demo.traccar.org/api
# Use either API Key OR Username/Password
TRACCAR_API_KEY=your_traccar_api_key
# OR
TRACCAR_USERNAME=your_username
TRACCAR_PASSWORD=your_password

# General Configuration
DEBUG=false
API_TIMEOUT=10000
NODE_ENV=development

# Server Configuration
PORT=5000
